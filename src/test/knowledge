    const fetch = require('node-fetch');
    async function upload(text) {
      return fetch('https://hasteb.in/documents', {
        method: 'POST',
        body: text,
      })
        .then((r) => r.json())
        .then((r) => {
          if (!r || !r.key) return null;
          return `https://hasteb.in/${r.key}`;
        })
        .catch(() => null);
    }
